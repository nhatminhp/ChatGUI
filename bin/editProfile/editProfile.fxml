         <children>
            <Label layoutX="37.0" layoutY="27.0" text="Username">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="37.0" layoutY="172.0" text="Date of birth">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="37.0" layoutY="78.0" text="Email">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="37.0" layoutY="124.0" text="Phone number">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <JFXTextField fx:id="EditUserNameTextField" layoutX="184.0" layoutY="21.0" prefHeight="31.0" prefWidth="234.0" />
