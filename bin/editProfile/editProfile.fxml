      </AnchorPane>
      <JFXButton fx:id="EditProfileImageButton" layoutX="740.0" layoutY="369.0" onAction="#editProfileImage" prefHeight="31.0" prefWidth="50.0" style="-fx-background-color: #66bbe2;" text="Edit" />
      <JFXButton fx:id="BackButton" layoutX="50.0" layoutY="50.0" onAction="#clickBackButton" prefHeight="29.0" prefWidth="50.0" text="Back">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
