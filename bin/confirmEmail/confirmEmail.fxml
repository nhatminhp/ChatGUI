<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ConfirmEmailPane" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="confirmEmail.ConfirmEmailController">
   <children>
      <Label layoutX="839.0" layoutY="164.0" text="Confirm your email" textFill="#009ff5">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label layoutX="676.0" layoutY="250.0" text="Enter your email address and we will send a confirmation code to your email">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <JFXTextField fx:id="ConfirmEmailTextField" layoutX="839.0" layoutY="326.0" prefHeight="25.0" prefWidth="212.0" promptText="enter your email">
         <font>
            <Font size="14.0" />
         </font></JFXTextField>
      <JFXButton fx:id="NextConfirmEmailButton" layoutX="915.0" layoutY="427.0" onAction="#clickNextButton" prefHeight="29.0" prefWidth="60.0" style="-fx-background-color: #66bbe2;" text="Next" />
      <Label fx:id="NoticeError" layoutX="708.0" layoutY="394.0" prefHeight="20.0" prefWidth="474.0" textAlignment="CENTER" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <JFXButton fx:id="BackButton" layoutX="48.0" layoutY="51.0" onAction="#clickBackButton" prefHeight="50.0" prefWidth="50.0" text=".">
         <font>
            <Font size="13.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="ExitButton" layoutX="1839.0" layoutY="25.0" onAction="#Exit" opacity="0.4" prefHeight="50.0" prefWidth="50.0" text="." />
   </children>
</AnchorPane>
